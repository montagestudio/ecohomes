{
    "name": "eco-home",
    "version": "0.2.0",
	"private": true,
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:montagestudio/ecohomes.git"
    },
    "dependencies": {
        "montage": "montagejs/montage#master",
        "digit": "montagejs/digit#master",
        "glTF-webgl-viewer": "./node_modules/glTF-webgl-viewer",

        "q-connection": "0.5.3"
